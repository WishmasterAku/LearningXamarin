<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LearningXamarin.History">
    <ListView x:Name="postsListView"
              ItemSelected="postsListView_ItemSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Experience}"/>
                <!-- Creates a table that can be seen on the app, data pulled from DB Posts-->
            </DataTemplate>
            
        </ListView.ItemTemplate>
        
        
    </ListView>
    
</ContentPage>